<%- include('partials/header') %>
<%= console.log(typeof use) %>
<div class="jumbotron text-center">
  <div class="container">
    <table class="table table-dark table-hover">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col">Name</th>
          <th scope="col">From</th>
          <th scope="col">To</th>
          <th scope="col">Date</th>
          <th scope="col">time</th>
          <th scope="col">accept</th>
         
        </tr>
      </thead>
      <tbody>
        <% use.map((u,index)=>{ %>
        <tr>
          <th scop="row"> <input disabled type="text" name="ind" value="<%=u.id%>"></input></th>
          <td><%=u.name%></td>
          <td><%=u.to%></td>
          <td><%=u.for%></td>
          <td><%=u.date%></td>
          <td><%=u.time%></td>
          <td>
            <a href="/message"
              ><button
                type="button"
                class="btn btn-success"
                value="<%=u.id%>"
                id="accept"
              >
                >Accept
              </button>
            </a>
          </td>
          <td>
            
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>

<%- include('partials/footer') %>
